if params.len != 1 or params[0] == "-h" or params[0] == "--help" then exit("<b>Usage: "+program_path.split("/")[-1]+" [metaxploit.so location]</b>")
metaxploit = include_lib(params[0])

metalib = metaxploit.load("/lib/net.so")
if metalib then
	print("lib "+ metalib.lib_name)
end if
result = metaxploit.scan(metalib)
print(result[0])

result1 = metaxploit.scan_address(metalib, result[0])
print(result1)

print(result[1])

result2 = metaxploit.scan_address(metalib, result[1])
print(result2)

print(result[0])

result3 = metaxploit.scan_address(metalib, result[2])
print(result3)

